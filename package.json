        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.28.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-unicorn": "^48.0.1",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "gulp-changed": "^4.0.3",
        "gulp-clean-css": "^4.3.0",
        "gulp-cli": "^2.3.0",
        "gulp-concat": "^2.6.1",
        "gulp-debug": "^4.0.0",
        "gulp-exec": "^5.0.0",
