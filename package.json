        "http-server": "^14.1.1",
        "js-yaml": "^4.1.0",
        "postcss": "^8.4.27",
        "prettier": "^3.0.1",
        "rimraf": "^5.0.1",
        "rollup": "^3.28.0",
        "sass": "^1.65.1",
        "start-server-and-test": "^2.0.0",
        "stylelint": "^15.10.2",
        "stylelint-config-standard-scss": "^10.0.0",
        "stylelint-order": "^6.0.3",
        "vinyl-buffer": "^1.0.1",
