{
  "name": "gulp-firestarter-2020",
  "version": "0.1.0",
  "description": "Gulp starter kit 2020",
  "main": "gulpfile.esm.js",
  "scripts": {
    "start": "gulp",
    "build": "NODE_ENV=production gulp build",
    "gulp": "./node_modules/gulp-cli/bin/gulp.js",
    "combine-reports": "mochawesome-merge --reportDir cypress/reports/mocha > mochareports/report-$$(date +'%Y%m%d-%H%M%S').json",
    "generate-report": "marge mochareports/*.json -f report-$$(date +'%Y%m%d-%H%M%S') -o mochareports",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "preinstall": "npx check-engine",
    "caniuse:update": "npx browserslist@latest --update-db",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Martin Alker <hacktheplanet@mailbox.org>",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@sentry/browser": "^6.2.0",
    "@sentry/tracing": "^6.2.0",
    "@tailwindcss/typography": "^0.4.0",
    "tailwindcss": "^2.0.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/register": "^7.13.8",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/html": "^6.1.20",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "cross-spawn": "^7.0.3",
    "cypress": "^6.5.0",
    "cypress-multi-reporters": "^1.4.0",
    "eslint": "^7.20.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-unicorn": "^28.0.2",
    "esm": "^3.2.25",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-changed": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-debug": "^4.0.0",
    "gulp-exec": "^5.0.0",
    "gulp-extname": "^0.2.2",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-load-plugins": "^2.0.6",
    "gulp-nunjucks-render": "^2.2.3",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-stylelint": "^13.0.0",
    "gulp-uglify": "^3.0.2",
    "js-yaml": "^4.0.0",
    "mocha": "^8.3.0",
    "mocha-junit-reporters": "^1.23.6",
    "mochawesome": "^6.2.2",
