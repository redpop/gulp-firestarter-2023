        "@11ty/eleventy": "^2.0.1",
        "@babel/core": "^7.22.10",
        "@babel/preset-env": "^7.22.10",
        "@babel/register": "^7.22.5",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-multi-entry": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@rollup/stream": "^3.0.0",
